{
    "name": "manzadey/laravel-like",
    "description": "Allows Laravel Eloquent models to implement a 'favorite' or 'remember' or 'follow' feature.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Manzadey\\LaravelLike\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Manzadey\\LaravelLike\\Test\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Andrey Manzadey",
            "email": "andrey.manzadey@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/support": "^8.0|^9.0",
        "illuminate/database": "^8.0|^9.0",
        "php" : "^8.0|^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Manzadey\\LaravelLike\\LikeServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "0.1-x-dev"
        }
    }
}
