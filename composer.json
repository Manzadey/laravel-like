{
    "name": "manzadey/laravel-like",
    "description": "Allows Laravel Eloquent models to implement a 'like'",
    "keywords": [
        "Manzadey",
        "Laravel",
        "Like",
        "Dislike",
        "laravel-like"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Andrey Manzadey",
            "email": "andrey.manzadey@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "^8.0|^9.0",
        "illuminate/database": "^8.0|^9.0",
        "php": "^8.0|^8.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Manzadey\\LaravelLike\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Manzadey\\LaravelLike\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Manzadey\\LaravelLike\\LikeServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
